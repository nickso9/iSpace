


 {{#each user.feed}}
    <div class="card mt-2" id="{{this.id}}">
            <div class="card-header d-flex align-items-center">
                <img src="{{this.image}}" style="height: 50px;"><span class="card-user-name ml-2">{{this.username}}</span>
            </div>

        <div class="card-body">
            <h5 class="card-title bg-light pt-3 pb-3">{{this.headline}}</h5>
            <p class="card-text">{{this.text}}</p>
            <p class="created-by">{{this.createdAt}}</p>
        </div>
    </div>
  {{/each}}